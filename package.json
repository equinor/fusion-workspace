{
	"name": "workspace",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"first-time-setup": "npm i -g pnpm@8.0.0 && pnpm i && pnpm build",
		"build": "pnpm --parallel  -r build",
		"ci:build": "pnpm -r build",
		"build:prod": "pnpm -r build:prod",
		"documentation": "pnpx astro dev --root ./documentation"
	},
	"private": true,
	"packageManager": "pnpm@8.0.0",
	"dependencies": {
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@typescript-eslint/eslint-plugin": "~5.24.0",
		"@typescript-eslint/parser": "~5.24.0",
		"prettier": "^2.6.2",
		"typescript": "~5.0.2",
		"typescript-plugin-styled-components": "^2.0.0"
	},
	"workspaces": [
		"packages/**",
		"apps/**"
	]
}
