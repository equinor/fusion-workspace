{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/observable-proxy/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/web:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/observable-proxy",
				"tsConfig": "packages/observable-proxy/tsconfig.lib.json",
				"project": "packages/observable-proxy/package.json",
				"entryFile": "packages/observable-proxy/src/index.ts",
				"external": ["react/jsx-runtime"],
				"rollupConfig": "@nrwl/react/plugins/bundle-rollup",
				"compiler": "babel",
				"assets": [
					{
						"glob": "packages/observable-proxy/README.md",
						"input": ".",
						"output": "."
					}
				]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/observable-proxy/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/observable-proxy"],
			"options": {
				"jestConfig": "packages/observable-proxy/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
