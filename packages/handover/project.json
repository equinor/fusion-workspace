{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "packages/handover/src",
	"tags": [],
	"targets": {
		"serve": {
			"executor": "nx-plugin-vite:serve",
			"options": {
				"configFile": "packages/handover/vite.config.ts",
				"port": 3000,
				"host": false,
				"https": false
			}
		},
		"preview": {
			"executor": "nx-plugin-vite:preview",
			"options": {
				"configFile": "packages/handover/vite.config.ts"
			}
		},
		"build": {
			"executor": "nx-plugin-vite:build",
			"options": {
				"outDir": "dist",
				"configFile": "packages/handover/vite.config.ts",
				"watch": false,
				"write": true,
				"emitAtRootLevel": false,
				"manifest": true
			}
		}
	}
}
