{
	"name": "garden-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/garden-e2e/src",
	"projectType": "application",
	"targets": {
		"e2e": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"cypressConfig": "packages/garden-e2e/cypress.json",
				"devServerTarget": "garden:storybook",
				"testingType": "e2e"
			},
			"configurations": {
				"ci": {
					"devServerTarget": "garden:storybook:ci"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/garden-e2e/**/*.{js,ts}"]
			}
		}
	},
	"tags": [],
	"implicitDependencies": ["garden"]
}
